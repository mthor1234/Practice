package fibonacci;

public class Fibonacci_Sequence {
	
	int previousNumber = 0;
	
	
	public int sequence(int number){
		
		if(number == 0){
			System.out.print(number + ",");
			previousNumber = 0;
			sequence(1);
		}
		else{
			System.out.print(number + ",");
			sequence(number) + sequence(number);
			
		}
	}

}
