package polymorphism;

public class Corvette extends Car {

	public String shift = "Paddle Shifter";
	
	public Corvette(){
		super("VRRRRRRRRRR....Purrrrrrrrrr", "Red");
	}
	
	public void shift(){
		System.out.println("Shifting " + shift);
	}

	@Override
	protected void startEngine() {
		super.startEngine();
		System.out.println("Firing up that Corvette Engine Boi!");
	}
	
	
	
	
	

}
