package polymorphism;

public class Corvette extends Car {

	private String shift = "Paddle Shifter";
	
	public Corvette(){
		super("VRRRRRRRRRR....Purrrrrrrrrr", "Red", shift);
	}
	
	@Override
	public void shift(){
		System.out.println("Shifting " + shift);
	}

	@Override
	protected void startEngine() {
		super.startEngine();
	}
	
	
	
	
	

}
