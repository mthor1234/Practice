package polymorphism;

public class InheritanceTest {

	public static void main(String[] args) {
		Corvette corvette = new Corvette();
		Car volkswagon = new Volkswagon();

		corvette.startEngine();
		volkswagon.startEngine();
		
		corvette.shift();
		System.out.println(corvette.paddleShift);
		
		
	}

}
